{
  "name": "@dalisoft/frontend-react-tailwind",
  "version": "0.0.1",
  "description": "React.js, Tailwind, Bun and TanStack Start template for easy frontend bootstrap",
  "keywords": [
    "react",
    "tailwind",
    "bunjs",
    "tanstack-start",
    "typescript"
  ],
  "homepage": "https://github.com/dalisoft/frontend-react-tailwind#readme",
  "bugs": {
    "url": "https://github.com/dalisoft/frontend-react-tailwind/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/dalisoft/frontend-react-tailwind.git"
  },
  "license": "MIT",
  "author": "Davlatjon Shavkatov <opensource@dalisoft.uz>",
  "type": "module",
  "scripts": {
    "tsgo": "tsc",
    "check-types": "tsc --noEmit",
    "prepare": "lefthook install -f",
    "cleanup": "rm -rf .output dist .wrangler",
    "start": "bun run --bun .output/server/index.mjs",
    "dev": "bun run --bun vite dev",
    "build": "bun run cleanup && bun run --bun vite build && cp _redirects ./dist/client/",
    "serve": "bun run --bun vite preview",
    "test": "bun test",
    "deploy:cf": "bun run build && wrangler pages deploy",
    "cf-typegen": "wrangler types"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.8",
    "@evilmartians/lefthook": "^2.0.6",
    "@ls-lint/ls-lint": "^2.3.1",
    "@tailwindcss/vite": "^4.1.17",
    "@types/bun": "^1.3.3",
    "@types/react": "~19.2.7",
    "@types/react-dom": "~19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "dprint": "^0.50.2",
    "typescript": "^5.9.3",
    "vite": "^7.2.6",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^4.0.15",
    "wrangler": "^4.51.0"
  },
  "dependencies": {
    "@tanstack/react-router": "^1.139.14",
    "@tanstack/react-start": "^1.139.14",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tailwindcss": "^4.1.17"
  },
  "overrides": {
    "vite": "npm:rolldown-vite@latest"
  },
  "optionalDependencies": {
    "@vercel/kv": "^3.0.0"
  }
}
