{
  "name": "@dalisoft/frontend-react-tailwind",
  "version": "0.0.1",
  "description": "React.js, Tailwind, Bun and TanStack Start template for easy frontend bootstrap",
  "keywords": [
    "react",
    "tailwind",
    "bunjs",
    "tanstack-start",
    "typescript"
  ],
  "homepage": "https://github.com/dalisoft/frontend-react-tailwind#readme",
  "bugs": {
    "url": "https://github.com/dalisoft/frontend-react-tailwind/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/dalisoft/frontend-react-tailwind.git"
  },
  "license": "MIT",
  "author": "Davlatjon Shavkatov <opensource@dalisoft.uz>",
  "type": "module",
  "main": "src/App.tsx",
  "scripts": {
    "tsgo": "tsgo",
    "check-types": "tsgo --noEmit",
    "prepare": "lefthook install -f",
    "start": "bun run --bun server.ts",
    "dev": "bun --bun vite dev",
    "build": "bun --bun vite build",
    "serve": "bun --bun vite preview",
    "test": "bun test",
    "deploy": "bun run build && wrangler deploy",
    "cf-typegen": "wrangler types"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.5",
    "@cloudflare/vite-plugin": "^1.14.0",
    "@evilmartians/lefthook": "^2.0.3",
    "@ls-lint/ls-lint": "^2.3.1",
    "@netlify/vite-plugin-tanstack-start": "^1.1.12",
    "@tailwindcss/vite": "^4.1.17",
    "@typescript/native-preview": "^7.0.0-dev.20251111.1",
    "@vitejs/plugin-react": "^5.1.0",
    "dprint": "^0.50.2",
    "nitro": "^3.0.1-alpha.1",
    "vite": "^7.2.2",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^4.0.8",
    "wrangler": "^4.46.0"
  },
  "dependencies": {
    "@tanstack/react-router": "^1.135.2",
    "@tanstack/react-start": "^1.135.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tailwindcss": "^4.1.17"
  }
}
